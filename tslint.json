{
    "rules": {
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "spaces"
        ],
        "no-eval": true,
        "no-internal-module": true,
        "no-trailing-whitespace": true,
        "no-unsafe-finally": true,
        "no-var-keyword": true,
        "quotemark": [
            true,
            "double"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check",
            "allow-undefined-check"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "member-access": true,
        "cyclomatic-complexity": [
            true,
            15
        ],
        "no-empty-interface": true,
        "interface-name": [
            true,
            "always-prefix"
        ],
        "curly": true,
        "no-conditional-assignment": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "switch-default": true,
        "typeof-compare": true,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "prefer-const": true,
        "no-namespace": true,
        "no-consecutive-blank-lines": [
            true,
            1
        ],
        "typedef": [
            true,
            "member-variable-declaration",
            "parameter",
            "property-declaration"
        ],
        "no-console": [
            true,
            "debug",
            "info",
            "log",
            "time",
            "timeEnd",
            "trace"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "variable-name": [
            true,
            "check-format",
            "ban-keywords"
        ],
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "import-spacing": true,
        "one-variable-per-declaration": [
            true
        ],
        "align": [
            true,
            "parameters",
            "statements"
        ]
    }
}